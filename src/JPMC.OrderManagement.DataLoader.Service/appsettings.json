{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },

  "CloudWatchLogs": {
    "Enable": false,
    "LogGroup": "/JPMC/OrderManagement/DataLoader"
  },

  "Service": {
    "Environment": "local",
    "DynamoDbTableName": "jpmc.ordermanagement",
    "DownloadToFile": "./data.csv",
    "Job": {
      "BucketName": "jpmc-order-management-dev",
      "ObjectKey": "sample-data.csv"
    }
  }
}
