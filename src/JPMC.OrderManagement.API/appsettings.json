{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },

  "EndpointDefaults": {
    "Protocols": "Http1AndHttp2"
  },

  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:8080"
      }
    }
  },

  "AllowedHosts": "*",

  "XRay": {
    "Enable": false,
    "AWSXRayPlugins": "ECSPlugin"
  },

  "CloudWatchLogs": {
    "Enable": false
  },

  "Service": {
    "DynamoDbTableName": "jpmc.ordermanagement"
  }
}
